plugin_atune:
  url: "http://localhost:8099"
http_server:
  addr: "localhost:8099"
log:
  level: debug
  driver: file # 可选stdout和file。stdout：输出到终端控制台；file：输出到path下的指定文件。
  path: ./log/plugin_atune.log
  max_file: 1
  max_size: 10485760
mysql:
  host: localhost
  port: 3306
  user: ''
  password: ''
  database: PluginAtune
etcd:
  endpoints:
    - "localhost:2379"
  service_name: "atune-service"
  version: "3.0"
  dial_timeout: 5s
  menu_name: "机器调优"
  icon: "Setting"